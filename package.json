{
  "name": "@react-input/core",
  "version": "1.0.0",
  "author": "Nikolay Goncharuk <goncharuk.bro@gmail.com>",
  "description": "The core of the packages included in the `@react-input` scope.",
  "keywords": [
    "react",
    "react hook",
    "input",
    "input element",
    "input control",
    "input event"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GoncharukBro/react-input.git",
    "directory": "packages/core"
  },
  "bugs": {
    "url": "https://github.com/GoncharukBro/react-input/issues"
  },
  "homepage": "https://github.com/GoncharukBro/react-input/tree/main/packages/core#readme",
  "license": "MIT",
  "module": "dist/index.js",
  "main": "dist/node/index.cjs",
  "types": "dist/declaration/index.d.ts",
  "files": [
    "dist"
  ],
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/node/index.cjs"
    },
    "./*": {
      "import": [
        "./dist/*.js",
        "./dist/*"
      ],
      "require": [
        "./dist/node/*.cjs",
        "./dist/node/*"
      ]
    },
    "./*.js": {
      "import": [
        "./dist/*.js",
        "./dist/*"
      ],
      "require": [
        "./dist/node/*.cjs",
        "./dist/node/*"
      ]
    }
  },
  "typesVersions": {
    "*": {
      "dist/declaration/index.d.ts": [
        "dist/declaration/index.d.ts"
      ],
      "*": [
        "dist/declaration/*",
        "dist/declaration/*/index.d.ts"
      ]
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "write-entries": "node ../../scripts/writeEntries.js",
    "remove-dist": "node ../../scripts/removeDist.js",
    "build-rollup": "rollup --config rollup.config.js",
    "build": "npm run write-entries && npm run remove-dist && npm run build-rollup && node ../../scripts/consoleSuccess.js"
  },
  "peerDependencies": {
    "@types/react": ">=16.8",
    "react": ">=16.8",
    "react-dom": ">=16.8"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    }
  }
}
